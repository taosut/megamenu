<?xml version="1.0"?>
<config>
    <modules>
        <Ved_CatalogSearch>
            <version>1.0.0</version>
        </Ved_CatalogSearch>
    </modules>
    <frontend>
        <routers>
            <catalogsearch>
                <use>standard</use>
                <args>
                    <module>Ved_CatalogSearch</module>
                    <frontName>catalogsearch</frontName>
                </args>
            </catalogsearch>
        </routers>
        <layout>
            <updates>
                <catalogsearch>
                    <file>catalogsearch.xml</file>
                </catalogsearch>
            </updates>
        </layout>
    </frontend>
</config>
