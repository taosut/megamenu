<?xml version="1.0"?>
<config>
    <modules>
        <Ved_Authentication>
            <version>1.0.0</version>
        </Ved_Authentication>
    </modules>
    <frontend>
        <routers>
            <authentication>
                <use>standard</use>
                <args>
                    <module>Ved_Authentication</module>
                    <frontName>authentication</frontName>
                </args>
            </authentication>
        </routers>
    </frontend>
    <global>
        <helpers>
            <tek_authentication>
                <class>Ved_Authentication_Helper</class>
            </tek_authentication>
        </helpers>
    </global>
</config>