<?xml version="1.0"?>
<layout version="0.1.0">
    <adminhtml_coupon_index_index>
        <reference name="head">
            <action method="addJs">
                <script>productimport/jquery-1.11.3.min.js</script>
            </action>
            <action method="addJs">
                <script>productimport/noconflict.js</script>
            </action>
            <action method="addJs">
                <script>productimport/angular.min.js</script>
            </action>
            <action method="addJs">
                <script>purchase/bootstrap.min.js</script>
            </action>
            <action method="addCss">
                <stylesheet>purchase/css/bootstrap.modal.css</stylesheet>
            </action>
